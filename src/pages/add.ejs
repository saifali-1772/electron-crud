<%- include('header.ejs') -%>
        <div id="main-content">
            <h2>Add New Record</h2>
            <form class="post-form">
                <div class="form-group">
                    <label>Name</label>
                    <input type="text" name="sname"/>
                </div>
                <div class="form-group">
                    <label>Address</label>
                    <input type="text" name="saddress" />
                </div>
                <div class="form-group">
                    <label>Class</label>
                    <select name="class">
                        <option value="" selected disabled>Select Class</option>
                        <option value="#">cname</option>
                    </select>
                </div>
                <div class="form-group">
                    <label>Phone</label>
                    <input type="text" name="sphone" />
                </div>
                <input id="addBtn" class="submit" type="submit" value="Save" />
            </form>
        </div> 
    </div>
    <!-- <%- include('../JavaScript/renderers.js') -%> -->
    <script src="../JavaScript/renderers.js"></script>
</body>

</html>